#домашнє завдання 4.2 . Перевірка чи поліндром
def checked_palindrome(x):
    x == x[::-1]
    if x == x[::-1]:
        print ("True")
    else:
        print ("False")
print(checked_palindrome("ice"))

#варіант 2
word = str(input())
x = len(word)
i = 0
x = x - 1
k = 0
while x - i >= i:
    if word[x - i] == word[i]:
        i += 1
    else:
        k = 1
        break
if k == 1:
  print("no")
else:
  print("yes")
